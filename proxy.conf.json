{
  "carolOrganization": "ORG_NAME",
  "carolEnvironment": "ENV_NAME",
  "/api/*": {
    "target": "https://ORG_NAME.carol.ai",
    "secure": true,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/sql/*": {
    "target": "https://api.carol.ai/sql/v1/query",
    "secure": true,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/sql": "/"
    }
  }
}
